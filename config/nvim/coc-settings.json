{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.snippetStatusText": "\uf64d",
  "codeLens.enable": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.hintSign": "○",
  "diagnostic.infoSign": "●",
  "diagnostic.warningSign": "⚠",
  "elixir.pathToElixirLS": "~/Applications/elixir-ls/language_server.sh",
  "elixirLS.dialyzerEnabled": false,
  "emmet.includeLanguages": {
    "eelixir": "html",
    "vue": "html"
  },
  "emmet.showExpandedAbbreviation": "always",
  "explorer.width": 40,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [diagnosticError | 1] [indent][icon | 1] [filename omitCenter 1][modified][readonly]",
  "explorer.file.column.indent.indentLine": true,
  "git.addGBlameToVirtualText": true,
  "git.addedSign.text": "┃",
  "git.branchCharacter": "\uE725",
  "git.changeRemovedSign.text": "┃",
  "git.changedSign.text": "┃",
  "git.conflictedDecorator": "═",
  "git.removedSign.text": "┃",
  "git.stagedDecorator": "+",
  "git.topRemovedSign.text": "┏",
  "git.untrackedDecorator": "?",
  "highlight.disableLanguages": ["defx", "qf"],
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "suggest.enablePreview": true
}
